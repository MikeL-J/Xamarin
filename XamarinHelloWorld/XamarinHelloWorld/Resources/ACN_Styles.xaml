    <SolidColorBrush x:Key="SecondaryPressedBrush" Color="{StaticResource SecondaryPurple}" Opacity="0.8"/>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="{StaticResource SecondaryPurple}" Opacity="0.05" />
    <SolidColorBrush x:Key="MustangLightsBrush" Color="{StaticResource ArrowPurple}"/>
    <SolidColorBrush x:Key="BackgroundGreyBrush" Color="{StaticResource BackgroundGrey}" />
