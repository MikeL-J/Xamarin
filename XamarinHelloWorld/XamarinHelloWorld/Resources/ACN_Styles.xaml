                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:Double x:Key="PAGE_HEIGHT">1920</system:Double>
    <system:Double x:Key="PAGE_WIDTH">1080</system:Double>
    <system:String x:Key="DATA_FAQ_BUTTON_WIDTH">255</system:String>

    <ScaleTransform x:Key="ScaleUp2X" ScaleX="2" ScaleY="2" />

    --><!--Fonts--><!--
    <FontFamily x:Key="PrimaryFont">/bermuda-vend;component/assets/ACN/Fonts/#Graphik App</FontFamily>
    <FontFamily x:Key="SecondaryFont">/bermuda-vend;component/assets/ACN/Fonts/#Graphik App</FontFamily>
    <FontFamily x:Key="Title1Font">/bermuda-vend;component/assets/ACN/Fonts/#Graphik App</FontFamily>
    <FontFamily x:Key="Title2Font">/bermuda-vend;component/assets/MCBC/Fonts/#Graphik App</FontFamily>
    <FontFamily x:Key="CaptionFont">/bermuda-vend;component/assets/MCBC/Fonts/#Graphik App</FontFamily>
    --><!--Colors  --><!--
    <Color x:Key="PrimaryPurple">#a100ff</Color>
    <Color x:Key="PrimaryPurplePressed">#460073</Color>
    <Color x:Key="Accent">#7500c0</Color>
    <Color x:Key="SecondaryPurple">#7500c0</Color>
    <Color x:Key="SecondaryPurplePressed">#7500c0</Color>
    <Color x:Key="TertiaryColor">#004dff</Color>
    <Color x:Key="MediumGrey">#5a5a5a</Color>
    <Color x:Key="DisabledGrey">#969696</Color>
    <Color x:Key="DisabledButtonTextColor">#EAEAEA</Color>
    <Color x:Key="SecondaryDisabledGrey">#1A5A5A5A</Color>
    <Color x:Key="ArrowPurple">#3F0F6E</Color>
    <Color x:Key="BackgroundGrey">#f4f4f6</Color>
    <Color x:Key="White">#ffffff</Color>
    <Color x:Key="LightBlue">#008eff</Color>
    <Color x:Key="Black">#000000</Color>
    <Color x:Key="DarkGrey">#2f2f30</Color>
    <Color x:Key="PageBackground.Color">White</Color>
    <Color x:Key="PageBackground.Gradient.Bottom">#FFDCDCDC</Color>
    <Color x:Key="PageBackground.Gradient.Top">#FFE8E8EB</Color>
    <Color x:Key="FAQClosePanelBackground">#BFFFFFFF</Color>

    --><!--<SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}" />
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryPurple}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryPurple}" />
    <SolidColorBrush x:Key="CountdownBrush" Color="{StaticResource TertiaryColor}" />
    <SolidColorBrush x:Key="InfoBrush" Color="{StaticResource LightBlue}" />
    <SolidColorBrush x:Key="ModalBorderBrush" Color="#B3969696" />
    <SolidColorBrush x:Key="LightGreyBrush" Color="{StaticResource DisabledGrey}" />
    <SolidColorBrush x:Key="DisabledSecondaryBrush" Color="{StaticResource SecondaryDisabledGrey}" />
    <SolidColorBrush x:Key="DisabledButtonTextBrush" Color="{StaticResource DisabledButtonTextColor}" />
